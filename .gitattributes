# Images
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
*.webp filter=lfs diff=lfs merge=lfs -text

# Force LF line endings for packaging scripts only
packaging/linux/**/*.sh text eol=lf
packaging/linux/**/postinst text eol=lf
packaging/linux/**/prerm text eol=lf
packaging/linux/**/postrm text eol=lf
packaging/linux/**/*.py text eol=lf
packaging/linux/**/*.template text eol=lf
packaging/linux/**/*.service text eol=lf
packaging/linux/**/*.desktop text eol=lf
packaging/linux/**/audex text eol=lf
packaging/linux/**/audex-setup text eol=lf
packaging/linux/**/*.svg text eol=lf
